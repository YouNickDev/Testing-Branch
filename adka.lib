<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Exemplo</title>
</head>
<body>
    <h1>Bem-vindo ao site</h1>
    <div id="lista-container"></div>

    <script>
        // Lista de exemplo
        const minhaLista = [1, 2, 3, 4, 5];

        // Selecionando o container onde a lista ser√° exibida
        const listaContainer = document.getElementById('lista-container');

        // Criando um elemento <ul> para exibir a lista
        const ul = document.createElement('ul');

        // Adicionando os itens da lista ao <ul>
        minhaLista.forEach(item => {
            const li = document.createElement('li');
            li.textContent = item;
            ul.appendChild(li);
        });

        // Anexando o <ul> ao container
        listaContainer.appendChild(ul);
    </script>
</body>
</html>